{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "plugins": [
        "react",
        "prettier",
        "typescript"
    ],
    "rules": {
        "prettier/prettier": "error",
        "require-await": "error",
        "no-return-await": "error",
        "no-await-in-loop": "error"
    },
    "parser": "@babel/eslint-parser"
}